<view class="uni-list-chat" hover-class="{{!clickable&&!link?'':'uni-list-chat--hover'}}" data-event-opts="{{[['tap',[['onClick',['$event']]]]]}}" catchtap="__e"><view class="{{[(border)?'uni-list--border':'',(isFirstChild)?'uni-list-chat--first':'']}}"></view><view class="uni-list-chat__container"><view class="uni-list-chat__header-warp"><block s-if="{{avatarCircle||avatarList.length===0}}"><view class="{{['uni-list-chat__header',(avatarCircle)?'header--circle':'']}}"><image class="{{['uni-list-chat__header-image',(avatarCircle)?'header--circle':'']}}" src="{{avatarUrl}}" mode="aspectFill"></image></view></block><block s-else><view class="uni-list-chat__header"><block s-for="avatarList trackBy index" s-for-item="item" s-for-index="index"><view class="{{['uni-list-chat__header-box',computedAvatar]}}" style="{{'width:'+(imageWidth+'px')+';'+('height:'+(imageWidth+'px')+';')}}"><image class="uni-list-chat__header-image" style="{{'width:'+(imageWidth+'px')+';'+('height:'+(imageWidth+'px')+';')}}" src="{{item.url}}" mode="aspectFill"></image></view></block></view></block></view><block s-if="{{badgeText&&badgePositon==='left'}}"><view class="{{['uni-list-chat__badge','uni-list-chat__badge-pos',isSingle]}}"><text class="uni-list-chat__badge-text">{{badgeText==='dot'?'':badgeText}}</text></view></block><view class="uni-list-chat__content"><view class="uni-list-chat__content-main"><text class="uni-list-chat__content-title uni-ellipsis">{{title}}</text><view style="flex-direction:row;"><block s-if="{{isDraft}}"><text class="draft">[草稿]</text></block><text class="uni-list-chat__content-note uni-ellipsis">{{isDraft?$root.g0:note}}</text></view></view><view class="uni-list-chat__content-extra"><block s-if="{{$slots.default}}"><slot></slot></block><block s-else><text class="uni-list-chat__content-extra-text">{{time}}</text><block s-if="{{badgeText&&badgePositon==='right'}}"><view class="{{['uni-list-chat__badge',isSingle,badgePositon==='right'?'uni-list-chat--right':'']}}"><text class="uni-list-chat__badge-text">{{badgeText==='dot'?'':badgeText}}</text></view></block></block></view></view></view></view>