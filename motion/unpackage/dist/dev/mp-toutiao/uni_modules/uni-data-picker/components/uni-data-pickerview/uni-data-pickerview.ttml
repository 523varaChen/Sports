<view class="uni-data-pickerview"><scroll-view class="selected-area" scroll-x="true" scroll-y="false" show-scrollbar="{{false}}"><view class="selected-list"><block tt:for="{{selected}}" tt:for-item="item" tt:for-index="index"><block tt:if="{{item.text}}"><view data-event-opts="{{[['tap',[['handleSelect',[index]]]]]}}" class="{{((('selected-item')+' '+((index==selectedIndex)?'selected-item-active':''))+' '+((ellipsis)?'selected-item-text-overflow':''))}}" bindtap="__e"><text>{{item.text}}</text></view></block></block></view></scroll-view><view class="tab-c"><block tt:for="{{dataList}}" tt:for-item="child" tt:for-index="i" tt:key="i"><block tt:if="{{i==selectedIndex}}"><scroll-view class="list" scroll-y="{{true}}"><block tt:for="{{child}}" tt:for-item="item" tt:for-index="j"><view data-event-opts="{{[['tap',[['handleNodeClick',['$0',i,j],[[['dataList','',i],['','',j]]]]]]]}}" class="{{(('item')+' '+((!!item.disable)?'is-disabled':''))}}" bindtap="__e"><text class="item-text item-text-overflow">{{item[map.text]}}</text><block tt:if="{{selected.length>i&&item[map.value]==selected[i].value}}"><view class="check"></view></block></view></block></scroll-view></block></block><block tt:if="{{loading}}"><view class="loading-cover"><uni-load-more class="load-more" vue-id="d6ea480a-1" contentText="{{loadMore}}" status="loading" bind:__l="__l"></uni-load-more></view></block><block tt:if="{{errorMessage}}"><view class="error-message"><text class="error-text">{{errorMessage}}</text></view></block></view></view>