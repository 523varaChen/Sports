<view class="uni-data-tree"><view data-event-opts="{{[['tap',[['handleInput',['$event']]]]]}}" class="uni-data-tree-input" onTap="__e"><block a:if="{{$slots.$default}}"><slot options="{{options}}" data="{{inputSelected}}" error="{{errorMessage}}"></slot></block><block a:else><view class="{{(('input-value')+' '+((border)?'input-value-border':''))}}"><block a:if="{{errorMessage}}"><text class="selected-area error-text">{{errorMessage}}</text></block><block a:else><block a:if="{{loading&&!isOpened}}"><view class="selected-area"><uni-load-more class="load-more" vue-id="c92b6608-1" contentText="{{loadMore}}" status="loading" onVueInit="__l"></uni-load-more></view></block><block a:else><block a:if="{{inputSelected.length}}"><scroll-view class="selected-area" scroll-x="true"><view class="selected-list"><view class="selected-item" a:for="{{inputSelected}}" a:for-item="item" a:for-index="index" a:key="index"><text class="text-color">{{item.text}}</text><block a:if="{{index<inputSelected.length-1}}"><text class="input-split-line">{{split}}</text></block></view></view></scroll-view></block><block a:else><text class="selected-area placeholder">{{placeholder}}</text></block></block></block><block a:if="{{clearIcon&&!readonly&&inputSelected.length}}"><view data-event-opts="{{[['tap',[['clear',['$event']]]]]}}" class="icon-clear" catchTap="__e"><uni-icons vue-id="c92b6608-2" type="clear" color="#c0c4cc" size="24" onVueInit="__l"></uni-icons></view></block><block a:if="{{(!clearIcon||!inputSelected.length)&&!readonly}}"><view class="arrow-area"><view class="input-arrow"></view></view></block></view></block></view><block a:if="{{isOpened}}"><view data-event-opts="{{[['tap',[['handleClose',['$event']]]]]}}" class="uni-data-tree-cover" onTap="__e"></view></block><block a:if="{{isOpened}}"><view class="uni-data-tree-dialog"><view class="uni-popper__arrow"></view><view class="dialog-caption"><view class="title-area"><text class="dialog-title">{{popupTitle}}</text></view><view data-event-opts="{{[['tap',[['handleClose',['$event']]]]]}}" class="dialog-close" onTap="__e"><view class="dialog-close-plus" data-id="close"></view><view class="dialog-close-plus dialog-close-rotate" data-id="close"></view></view></view><data-picker-view class="picker-view" vue-id="c92b6608-3" localdata="{{localdata}}" preload="{{preload}}" collection="{{collection}}" field="{{field}}" orderby="{{orderby}}" where="{{where}}" step-searh="{{stepSearh}}" self-field="{{selfField}}" parent-field="{{parentField}}" managed-mode="{{true}}" map="{{map}}" ellipsis="{{ellipsis}}" ref="__r" data-ref="pickerView" value="{{dataValue}}" data-event-opts="{{[['^change',[['onchange']]],['^datachange',[['ondatachange']]],['^nodeclick',[['onnodeclick']]],['^input',[['__set_model',['','dataValue','$event',[]]]]]]}}" onChange="__e" onDatachange="__e" onNodeclick="__e" onInput="__e" onVueInit="__l"></data-picker-view></view></block></view>