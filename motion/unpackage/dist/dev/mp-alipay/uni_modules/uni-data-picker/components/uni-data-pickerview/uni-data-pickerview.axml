<view class="uni-data-pickerview"><scroll-view class="selected-area" scroll-x="true" scroll-y="false" show-scrollbar="{{false}}"><view class="selected-list"><block a:for="{{selected}}" a:for-item="item" a:for-index="index"><block a:if="{{item.text}}"><view data-event-opts="{{[['tap',[['handleSelect',[index]]]]]}}" class="{{((('selected-item')+' '+((index==selectedIndex)?'selected-item-active':''))+' '+((ellipsis)?'selected-item-text-overflow':''))}}" onTap="__e"><text>{{item.text}}</text></view></block></block></view></scroll-view><view class="tab-c"><block a:for="{{dataList}}" a:for-item="child" a:for-index="i" a:key="i"><block a:if="{{i==selectedIndex}}"><scroll-view class="list" scroll-y="{{true}}"><view data-event-opts="{{[['tap',[['handleNodeClick',['$0',i,j],[[['dataList','',i],['','',j]]]]]]]}}" class="{{(('item')+' '+((!!item.disable)?'is-disabled':''))}}" onTap="__e" a:for="{{child}}" a:for-item="item" a:for-index="j"><text class="item-text item-text-overflow">{{item[map.text]}}</text><block a:if="{{selected.length>i&&item[map.value]==selected[i].value}}"><view class="check"></view></block></view></scroll-view></block></block><block a:if="{{loading}}"><view class="loading-cover"><uni-load-more class="load-more" vue-id="d6fb701c-1" contentText="{{loadMore}}" status="loading" onVueInit="__l"></uni-load-more></view></block><block a:if="{{errorMessage}}"><view class="error-message"><text class="error-text">{{errorMessage}}</text></view></block></view></view>